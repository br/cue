#!/bin/bash

if [[ "$#" > 0 ]]; then
  CUE="$1"; shift
else
  CUE="$(cd -P -- "$(dirname -- "${BASH_SOURCE}")/.." && pwd -P)"
fi  

source "$CUE/bin/_nim"
source "$CUE/bin/_bindings"
source "$CUE/bin/_solarized"
source "$CUE/bin/_prompt"

: ${PS1_START:='--'}
: ${PS1_LEFT:='${prn_pwd}'}
: ${PS1_RIGHT:='${prn_ssh}'}
: ${PS1_END:=' --'}
